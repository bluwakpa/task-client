.setState(data), 600)\n  }\n\n  renderNavRoutes() {\n    const { tasks, categories } = this.state\n    return (\n      <>\n        {['/', '/category/:categoryId'].map(path =>\n          <Route\n            exact\n            key={path}\n            path={path}\n            render={routeProps =>\n              <TaskListNav\n              categories={categories}\n                tasks={tasks}\n                {...routeProps}\n              />\n            }\n          />\n        )}\n        <Route\n          path='/task/:taskId'\n          render={routeProps => {\n            const { taskId } = routeProps.match.params\n            const task = findTask(tasks, taskId) || {}\n            const category = findCategory(categories, task.categoryId)\n            return (\n              <TaskPageNav\n                {...routeProps}\n                category={category}\n              />\n            )\n          }}\n        />\n        <Route\n          path='/add-category'\n          component={TaskPageNav}\n        />\n        <Route\n          path='/add-task'\n          component={TaskPageNav}\n        />\n      </>\n    )\n  }\n\n  renderMainRoutes() {\n    const { tasks, categories } = this.state\n    return (\n      <>\n        {['/', '/category/:categoryId'].map(path =>\n          <Route\n            exact\n            key={path}\n            path={path}\n            render={routeProps => {\n              const { categoryId } = routeProps.match.params\n              const getTasksForCategory = getTasksForCategory(tasks, categoryId)\n              return (\n                <TaskListMain\n                  {...routeProps}\n                  tasks={getTasksForCategory}\n                />\n              )\n            }}\n          />\n        )}\n        <Route\n          path='/task/:taskId'\n          render={routeProps => {\n            const { taskId } = routeProps.match.params\n            const task = findTask(tasks, taskId)\n            return (\n              <TaskPageMain\n                {...routeProps}\n                task={task}\n              />\n            )\n          }}\n        />\n        <Route\n          path='/add-category'\n          component={AddCategory}\n        />\n        <Route\n          path='/add-task'\n          render={routeProps => {\n            return (\n              <AddTask\n                {...routeProps}\n                categories={categories}\n              />\n            )\n          }}\n        />\n      </>\n    )\n  }\n\n  render() {\n    return (\n      <div className='App'>\n        <nav className='App__nav'>\n          {this.renderNavRoutes()}\n        </nav>\n        <header className='App__header'>\n          <h1>\n            <Link to='/'>Task</Link>\n            {' '}\n            <FontAwesomeIcon icon='check-double' />\n          </h1>\n        </header>\n        <main className='App__main'>\n          {this.renderMainRoutes()}\n        </main>\n      </div>\n    )\n  }\n}\n\nexport default App","/Users/bluwakpa/workspace/task-client/src/Task.js",[],"/Users/bluwakpa/workspace/task-client/src/TaskList.js",["86","87","88"],"/Users/bluwakpa/workspace/task-client/src/CategoryNav.js",["89","90"],"/Users/bluwakpa/workspace/task-client/src/data.js",["91"],"export default {\n  \"categories\": [\n    {\n      \"id\": \"b0715efe-ffaf-11e8-8eb2-f2801f1b9fd1\",\n      \"name\": \"Personal\"\n    },\n    {\n      \"id\": \"b07161a6-ffaf-11e8-8eb2-f2801f1b9fd1\",\n      \"name\": \"Work\"\n    },\n    {\n      \"id\": \"b07162f0-ffaf-11e8-8eb2-f2801f1b9fd1\",\n      \"name\": \"Chores\"\n    }\n  ],\n  \"tasks\": [\n    {\n      \"id\": \"cbc787a0-ffaf-11e8-8eb2-f2801f1b9fd1\",\n      \"content\": \"Excercise\",\n      \"modified\": \"2021-01-03T00:00:00.000Z\",\n      \"complete\": false,\n      \"categoryId\": \"b0715efe-ffaf-11e8-8eb2-f2801f1b9fd1\",\n    },\n    {\n      \"id\": \"d26e0034-ffaf-11e8-8eb2-f2801f1b9fd1\",\n      \"content\": \"Debug React app\",\n      \"modified\": 